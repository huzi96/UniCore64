From ad81f4f251602ffff9efa698a7313a2c92095b50 Mon Sep 17 00:00:00 2001
From: LiangRongrong <Liangrongrong@mprc.pku.edu.cn>
Date: Mon, 5 Dec 2011 11:01:07 +0800
Subject: [PATCH 117/641] UniCore64: Modify cp0 to p0 in arch asm-mmuops.h

Signed-off-by: Liang Rongrong <liangrongrong@mprc.pku.edu.cn>
---
 arch/unicore64/include/arch/asm-mmuops.h |    8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/unicore64/include/arch/asm-mmuops.h b/arch/unicore64/include/arch/asm-mmuops.h
index 8dc3253..55408fa 100644
--- a/arch/unicore64/include/arch/asm-mmuops.h
+++ b/arch/unicore64/include/arch/asm-mmuops.h
@@ -8,7 +8,7 @@
  */
 __ASMMACRO_WRAP(.macro uc64_invalidate_dcache, rt;
 			dmovl	&rt, #0;
-			movc	cp0.c6, &rt, #8;
+			movc	p0.c6, &rt, #8;
 		.endm)
 
 /**
@@ -16,7 +16,7 @@ __ASMMACRO_WRAP(.macro uc64_invalidate_dcache, rt;
  */
 __ASMMACRO_WRAP(.macro uc64_flush_dcache, rt;
 			dmovl	&rt, #0;
-			movc	cp0.c6, &rt, #12;
+			movc	p0.c6, &rt, #12;
 		.endm)
 
 /**
@@ -24,7 +24,7 @@ __ASMMACRO_WRAP(.macro uc64_flush_dcache, rt;
  */
 __ASMMACRO_WRAP(.macro uc64_invalidate_icache, rt;
 			dmovl	&rt, #0;
-			movc	cp0.c7, &rt, #0;
+			movc	p0.c7, &rt, #0;
 		.endm)
 
 /**
@@ -32,7 +32,7 @@ __ASMMACRO_WRAP(.macro uc64_invalidate_icache, rt;
  */
 __ASMMACRO_WRAP(.macro uc64_invalidate_tlb, rt;
 			dmovl	&rt, #0;
-			movc	cp0.c5, &rt, #24;
+			movc	p0.c5, &rt, #24;
 		.endm)
 
 #endif /* __UNICORE64_ARCH_ASM_MMUOPS_H__ */
-- 
