+++ b/arch/unicore64/include/arch/hwdef-copro.h
@@ -58,12 +58,12 @@
  * CP0_CTRLREG_WRITEBACK:	Dcache write mechanism: write back
  * \\\\lt:/programlisting\\\\gt:
  */
-#define CP0_CTRLREG_MMU			FIELD(1, 1, 0)
-#define CP0_CTRLREG_ALIGN		FIELD(1, 1, 1)
