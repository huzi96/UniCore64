From beb29b35fb5fb4d12bb6df90d7e89dbfb5a93000 Mon Sep 17 00:00:00 2001
From: Guan Xuetao <gxt@mprc.pku.edu.cn>
Date: Wed, 18 Jan 2012 15:00:39 +0800
Subject: [PATCH 248/641] UniCore64: add a blank after uc64_debug_putdata

Signed-off-by: Guan Xuetao <gxt@mprc.pku.edu.cn>
---
 arch/unicore64/include/arch/asm-debug.h |    3 +++
 1 file changed, 3 insertions(+)

diff --git a/arch/unicore64/include/arch/asm-debug.h b/arch/unicore64/include/arch/asm-debug.h
index 7ce7919..fd97700 100644
--- a/arch/unicore64/include/arch/asm-debug.h
+++ b/arch/unicore64/include/arch/asm-debug.h
@@ -25,6 +25,9 @@ __ASMMACRO_WRAP(.macro	uc64_debug_putdata, rdata, rt1, rt2;
 			mov	r0, #0;
 			mov	r1, &rdata;
 			bkpt;
+			mov	r0, #1;
+			mov	r1, #0x20;
+			bkpt;
 			mov	r0, &rt1;
 			mov	r1, &rt2;
 		.endm)
