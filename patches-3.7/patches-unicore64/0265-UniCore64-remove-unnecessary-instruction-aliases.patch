From c36d566982f36ec454204255a468a53bed113649 Mon Sep 17 00:00:00 2001
From: Guan Xuetao <gxt@mprc.pku.edu.cn>
Date: Wed, 25 Jan 2012 16:30:22 +0800
Subject: [PATCH 265/641] UniCore64: remove unnecessary instruction aliases

Signed-off-by: Guan Xuetao <gxt@mprc.pku.edu.cn>
---
 arch/unicore64/include/arch/asm-common.h |    5 -----
 arch/unicore64/lib/debug.S               |    2 +-
 2 files changed, 1 insertion(+), 6 deletions(-)

diff --git a/arch/unicore64/include/arch/asm-common.h b/arch/unicore64/include/arch/asm-common.h
index 76ef89a..a29fb74 100644
--- a/arch/unicore64/include/arch/asm-common.h
+++ b/arch/unicore64/include/arch/asm-common.h
@@ -3,11 +3,6 @@
 
 #include <linux/stringify.h>
 
-#ifdef __ASSEMBLY__
-#define csub		cmpsub
-#define dcsub		dcmpsub
-#endif /* __ASSEMBLY__ */
-
 /*
  * __ASMMACRO_WRAP is ONLY used for assembly macro definitions.
  * Then this macro could be used in both assembly files and c files.
diff --git a/arch/unicore64/lib/debug.S b/arch/unicore64/lib/debug.S
index fba663c..86527e4 100644
--- a/arch/unicore64/lib/debug.S
+++ b/arch/unicore64/lib/debug.S
@@ -13,7 +13,7 @@ ENTRY(uc64_debug_puts)
 	dmov	r1, #UC64_DEBUG_PUTS_MAX
 
 1001:	ldb.w	r2, [r0]+, #1
-	dcsub.a r2, #0
+	dcmpsub.a	r2, #0
 	beq	1003f
 
 	__putchar	r2, r14, r15
-- 
1.7.9.5

