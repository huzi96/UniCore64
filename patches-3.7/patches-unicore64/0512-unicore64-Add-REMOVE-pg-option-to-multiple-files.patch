From c7b884a9fb242b283d60ce379ccff721768ac0b2 Mon Sep 17 00:00:00 2001
From: Guan Xuetao <gxt@mprc.pku.edu.cn>
Date: Mon, 31 Dec 2012 14:13:23 +0800
Subject: [PATCH 512/641] unicore64: Add REMOVE-pg option to multiple files

Signed-off-by: Guan Xuetao <gxt@mprc.pku.edu.cn>
---
 arch/unicore64/kernel/Makefile |    1 +
 arch/unicore64/mm/Makefile     |    4 ++++
 2 files changed, 5 insertions(+)

diff --git a/arch/unicore64/kernel/Makefile b/arch/unicore64/kernel/Makefile
index a43c64e..c6c231e 100644
--- a/arch/unicore64/kernel/Makefile
+++ b/arch/unicore64/kernel/Makefile
@@ -6,6 +6,7 @@
 ccflags-$(CONFIG_DEBUG_MCOUNT)	:= -pg
 CFLAGS_REMOVE_early_printk.o	:= -pg
 CFLAGS_REMOVE_time.o		:= -pg
+CFLAGS_REMOVE_process.o		:= -pg
 
 obj-y				:= head.o entry.o switch_to.o
 obj-y				+= irq.o
diff --git a/arch/unicore64/mm/Makefile b/arch/unicore64/mm/Makefile
index e3c7e6d..1633838 100644
--- a/arch/unicore64/mm/Makefile
+++ b/arch/unicore64/mm/Makefile
@@ -1,4 +1,8 @@
 ccflags-$(CONFIG_DEBUG_MCOUNT)	:= -pg
+CFLAGS_REMOVE_fault.o		:= -pg
+CFLAGS_REMOVE_mmu.o		:= -pg
+CFLAGS_REMOVE_pgtable.o		:= -pg
+CFLAGS_REMOVE_tlb.o		:= -pg
 
 obj-y				:= fault.o ioremap.o init.o
 obj-y				+= mmu.o pgtable.o tlb.o
-- 
