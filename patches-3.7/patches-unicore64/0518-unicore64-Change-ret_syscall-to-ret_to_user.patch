From 5f47b7bae323d2263de7dd16f8c7b75d17fca768 Mon Sep 17 00:00:00 2001
From: Guan Xuetao <gxt@mprc.pku.edu.cn>
Date: Mon, 31 Dec 2012 17:33:01 +0800
Subject: [PATCH 518/641] unicore64: Change ret_syscall to ret_to_user

Signed-off-by: Guan Xuetao <gxt@mprc.pku.edu.cn>
---
 arch/unicore64/kernel/entry.S |   10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/unicore64/kernel/entry.S b/arch/unicore64/kernel/entry.S
index ac9b7d3..8804c054 100644
--- a/arch/unicore64/kernel/entry.S
+++ b/arch/unicore64/kernel/entry.S
@@ -74,13 +74,13 @@
  */
 ENTRY(ret_from_fork)
 	call		schedule_tail
-	b		ret_syscall
+	b		ret_to_user
 ENDPROC(ret_from_fork)
 
 ENTRY(ret_from_kthread)
 	call		schedule_tail
 	dmov		r0, r17
-	adr		lr, ret_syscall
+	adr		lr, ret_to_user
 	jump		r18
 ENDPROC(ret_from_kthread)
 
@@ -116,13 +116,13 @@ ENTRY(__vec_jepriv)
 	dadd		sp, sp, #16
 	std.w		r0, [sp+], #0
 
-ret_syscall:
+ret_to_user:
 	__irq_disable
 	__thread_info	r17
 	ldw		r18, [r17+], #THREAD_INFO_FLAGS
 	and		r0, r18, #(1 << TIF_NEED_RESCHED)
 	cmpsub.a	r0, #0
-	adr		lr, ret_syscall
+	adr		lr, ret_to_user
 	bne		schedule
 	and		r0, r18, #(1 << TIF_NOTIFY_RESUME | 1 << TIF_SIGPENDING)
 	cmpsub.a	r0, #0
@@ -199,7 +199,7 @@ ENTRY(__vec_int_itimer)
 	ldd		r17, [sp+], #240	@ get bsr
 	dand		r17, r17, #7
 	cmpsub.a	r17, #1
-	beq		ret_syscall
+	beq		ret_to_user
 
 	__context_restore
 	eret
-- 
1.7.9.5

